  "experimental": {
    "context_compaction_threshold": 50,
    "disable_paste_summary": true,
    "skip_models_refresh": true,
    "cache_command_markdown_files": false,
  },
  "tui": {
    "sidebar_overlay": false,
    "messages_limit": 1000,
    "session_list_limit": 250,
    "diff_style": "stacked",
    "recent_models_count": 10,
    "display_message_tps": true,
    "no_sidebar_auto": true,
  },
  "keybinds": {
    "leader": "f1",
    "command_list": "alt+x",
    "history_next": "ctrl+n",
    "history_previous": "ctrl+p",
    "messages_copy": "ctrl+w",
    "messages_last_user": "alt+l",
    "messages_next": "alt+down",
    "messages_previous": "alt+up",
    "model_cycle_favorite": "ctrl+k",
    "model_cycle_favorite_reverse": "ctrl+j",
    "model_cycle_recent": "alt+k",
    "model_cycle_recent_reverse": "alt+j",
    "model_list": "alt+m",
    "session_child_cycle": "ctrl+right",
    "session_child_cycle_reverse": "ctrl+left",
    "session_fork": "ctrl+alt+f",
    "session_list": "ctrl+s",
    "session_new": "alt+n",
    "session_parent": "ctrl+up",
    "session_rename": "ctrl+r",
    "session_timeline": "ctrl+g",
    "sidebar_toggle": "ctrl+b",
    "theme_list": "ctrl+t",
    "tool_details": "ctrl+t",
    "username_toggle": "alt+u",

